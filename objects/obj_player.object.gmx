            <kind>1</kind>
            <string>/// Sprite and Ghosting Draw Event
var counter = 0;

// If player presses the boost button and the boost bar is greater than zero then show ghost trail effect
if (gamepad_button_value(0,gp_shoulderrb) &gt; 0)
    {                                
    boost = gamepad_button_value(0,gp_shoulderrb) * 2;  // set boost value to equal trigger value times 2
    if (boost_expended = false) // if boost is not expended then player can use boost
        {            
        // if boost bar is greater than zero, allow player to boost
        if (boost_bar &gt; 0)
            {
            draw_sprite_ext(sprite_index,image_index,x,y,1,1,image_angle,16777215,1)
            // Draw trail and update alpha
            for (i=0; i&lt;spr_trail_total; i+=1)
                {
                draw_sprite_ext(sprite_index,image_index,spr_trail_x[i],spr_trail_y[i],1,1,image_angle,c_white,spr_trail_a[i])
                spr_trail_a[i]-=0.06
                }            
            }
        
        // If boost is expended, draw the ghost trail for a brief moment
        if (boost_expended = true)
            {       
            if(counter &lt;= 90)
                {
                draw_sprite_ext(sprite_index,image_index,x,y,1,1,image_angle,16777215,1);
                // Draw trail and update alpha
                for (i=0; i &lt; spr_trail_total; i+=1)
                    {
                    draw_sprite_ext(sprite_index,image_index,spr_trail_x[i],spr_trail_y[i],1,1,image_angle,c_white,spr_trail_a[i]);
                    spr_trail_a[i]-=0.06;
                    }
                counter++;
                }            
            }    
            
        }
    

        

    // Else do not draw the ghost trail when moving
    else
        {
        // !!!! draw_sprite_ext(spr_player,image_index,x,y,1,1,image_angle,-1,1);
        draw_sprite_ext(spr_3d_nanite,image_index,x,y,1,1,image_angle,-1,1);
        }
    }
    
// But if player is hurt then draw the player is hurt
if (is_hurt = true)
    {
    sprite_index = spr_player_hurt;
    draw_sprite_ext(spr_player_hurt,image_index,x,y,1,1,image_angle,-1,1);
    }
// Else the player is not hurt
if (is_hurt = false)
    {
    // !!!! sprite_index = spr_player;
    sprite_index = spr_3d_nanite;
    // !!!!! draw_sprite_ext(spr_player,image_index,x,y,1,1,image_angle,-1,1);
    draw_sprite_ext(spr_3d_nanite,image_index,x,y,1,1,image_angle,-1,1);
    }
</string>
          </argument>
        </arguments>
