      <action>
        <libid>1</libid>
        <id>603</id>
        <kind>7</kind>
        <userelative>0</userelative>
        <isquestion>0</isquestion>
        <useapplyto>-1</useapplyto>
        <exetype>2</exetype>
        <functionname></functionname>
        <codestring></codestring>
        <whoName>self</whoName>
        <relative>0</relative>
        <isnot>0</isnot>
        <arguments>
          <argument>
            <kind>1</kind>
            <string>///Player movement and boost controls
nanite_speed = 10  // Set the Nanite base speed
instance_change(obj_player,0);

// But if player is hurt then draw the player is hurt
if (is_hurt = true)
    {
    sprite_index = spr_player_hurt;
    //draw_sprite_ext(spr_player_hurt,image_index,x,y,1,1,image_angle,-1,1);
    }
// Else the player is not hurt
if (is_hurt = false)
    {
    //sprite_index = spr_player;//////////////////////
    sprite_index = spr_3d_nanite;
    //draw_sprite_ext(spr_player,image_index,x,y,1,1,image_angle,-1,1);
    }

// are controllers supported om this platform?
if (gamepad_is_supported())
    {         
    // controller plugged in?
    if (gamepad_is_connected(0))                                                 
        {
        x_axis = gamepad_axis_value(0,gp_axislh)
        y_axis = gamepad_axis_value(0,gp_axislv)

        // left stick moves object
        image_angle = point_direction(0,0,gamepad_axis_value(0,gp_axislh),gamepad_axis_value(0,gp_axislv));
        
        // save image_angle; used to set the point direction of the player when player stops moving
        last_image_angle = image_angle;  

        // if right trigger is pressed, run boost routine
        if (gamepad_button_value(0,gp_shoulderrb) &gt; 0)
            {                                
            boost = gamepad_button_value(0,gp_shoulderrb) * 6;  // set boost value to equal trigger value (max value is 6)
            if (boost_expended = false) // if boost is not expended then player can use boost
                {            
                // if boost bar is greater than zero, allow player to boost
                if (boost_bar &gt; 0)
                    {
                    // !!!!  sprite_index = spr_player_boost_ghost; //change player object to object with thrust animation
                    sprite_index = spr_3d_nanite; //change player object to object with thrust animation
                    nanite_speed *= boost;  // add boost to the nanite_speed
                    if (!(audio_is_playing(snd_player_boost)))
                        {
                        var snd = audio_play_sound(snd_player_boost,0,false);
                        audio_sound_gain(snd,.5,0);                        
                        }                
                    
                    //if boost value is greater than 6, set max nanite speed; prevents player from moving too fast when boosting
                    if (nanite_speed &gt;= 30)
                        {
                        nanite_speed = 30;
                        x += gamepad_axis_value(0,gp_axislh) * (nanite_speed);                        
                        y += gamepad_axis_value(0,gp_axislv) * (nanite_speed);
                        boost_bar -= boost                        
                        }
                    //else allow the nanite speed to be boosted up to a boost value below 6
                    else
                        {
                        ///--&gt; nanite_speed = 5 * (boost + 1); //15 is the max value; pressing the right trigger produces a value between 0.1 to 1.0
                        x += gamepad_axis_value(0,gp_axislh) * (nanite_speed);
                        y += gamepad_axis_value(0,gp_axislv) * (nanite_speed);
                        boost_bar -= boost
                        }
                    }
                if (boost_bar &lt;= 0)
                    {
                    boost_expended = true;
                    if (!(audio_is_playing(snd_boost_reversed)))
                        {
                        audio_stop_sound(snd_player_boost);
                        var snd = audio_play_sound(snd_boost_reversed,0,false);
                        audio_sound_gain(snd,.5,0);                        
                        } 
                    // Decelerate the boost gradually
                    while(nanite_speed &gt; 1)
                        {       
                        nanite_speed = nanite_speed - (nanite_speed * 0.25);
                        x += gamepad_axis_value(0,gp_axislh) * (nanite_speed);
                        y += gamepad_axis_value(0,gp_axislv) * (nanite_speed);
                        }
                    }
                }


            //else boost_bar is less than zero and boost is expended (because it failed the 'if' statement above')
            else
                {              
                x += gamepad_axis_value(0,gp_axislh) * (nanite_speed);
                y += gamepad_axis_value(0,gp_axislv) * (nanite_speed);
                }
                
            }
 

        else // If right trigger is not pressed (boost button) then move player without boost   
            {
            // !!! sprite_index = spr_player; // if right-trigger not pressed or is released change sprite to player without thrust animation
            sprite_index = spr_3d_nanite
            audio_stop_sound(snd_player_boost);
            
            // If the boost bar is 10 or higher then set boost_expended to false (allows player to use boost)
            if (boost_bar &gt;= 10)
                {
                boost_expended = false;
                if (boost_bar &gt;= 95) //&lt;--- Set Boost Bar Max Level normally is 95 (shows as 100 in game)
                    {
                    boost_bar = 95; //&lt;--- Set Boost Bar Max Level normally is 95 (shows as 100 in game)
                    }
                }                
            if (boost_bar &lt;= 0)
                {
                boost_expended = true;
                boost_bar += 1; //refill boost bar
                }
            else 
                {                
                boost_bar += 5; //refill boost bar
                }

            x += gamepad_axis_value(0,gp_axislh) * (nanite_speed);
            y += gamepad_axis_value(0,gp_axislv) * (nanite_speed);
            }


        // left stick button resets position (jumps back to starting position)
        if (gamepad_button_check_pressed(0,gp_stickl))     
            {
            x = xstart;
            y = ystart;
            }
        
            
        }
    }


</string>
          </argument>
