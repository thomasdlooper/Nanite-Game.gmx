            <kind>1</kind>
            <string>/// Player shoot button
// are controllers supported om this platform?

if (gamepad_is_supported())
    {         
    // controller plugged in?
    if (gamepad_is_connected(0))                                                 
        {        
        if (gamepad_button_check(0,gp_face1))
            {
            
            player_is_firing = true;
            alarm[0] = fire_rate;
            
            if (player_is_firing = true)
                {
                
                //// Start ////
                // Setup coorinates for player's top weapon (nanite's left weapon)
                var aa,up,right,xx,yy;
    
                t_up = 13;             //how far up is the barrel from the origin of gun sprite        
                t_right = 8;        //how far to the right is the barrel from the origin of gun sprite - im just guessing these values you'll need to tweek them        
                t_xx = x;           //coordinates to spawn the bullet at        
                t_yy = y;          //to start off with lets set these to the guns position
                
                t_xx += lengthdir_x(t_right,image_angle);         //move the bullet spawn to the right (relative to how the gun is rotated) for x
                t_yy += lengthdir_y(t_right,image_angle);        //move the bullet spawn to the right (relative to how the gun is rotated) for y
                t_xx += lengthdir_x(t_up,image_angle+90);      //move the bullet spawn up (relative to how the gun is rotated) for x
                t_yy += lengthdir_y(t_up,image_angle+90);     //move the bullet spawn up (relative to how the gun is rotated) for y
            
                aa = instance_create(t_xx,t_yy,obj_player_bullet);             //create the bullet but use "aa" to remember it        
                aa.image_angle = image_angle;                //rotate the bullet sprite        
                aa.direction = image_angle;                       //set the direction the bullet will move        
                aa.speed = 20;                                             //set the speed the bullet will move
                
                // Setup coorinates for player's bottom weapon (nanite's right weapon)
                var bb,b_up,b_right,b_xx,b_yy;
    
                b_up = -13;             //how far up is the barrel from the origin of gun sprite        
                b_right = 8;        //how far to the right is the barrel from the origin of gun sprite - im just guessing these values you'll need to tweek them        
                b_xx = x;           //coordinates to spawn the bullet at        
                b_yy = y;          //to start off with lets set these to the guns position
                
                b_xx += lengthdir_x(b_right,image_angle);         //move the bullet spawn to the right (relative to how the gun is rotated) for x
                b_yy += lengthdir_y(b_right,image_angle);        //move the bullet spawn to the right (relative to how the gun is rotated) for y
                b_xx += lengthdir_x(b_up,image_angle+90);      //move the bullet spawn up (relative to how the gun is rotated) for x
                b_yy += lengthdir_y(b_up,image_angle+90);     //move the bullet spawn up (relative to how the gun is rotated) for y
                
                bb = instance_create(b_xx,b_yy,obj_player_bullet);  //create bullet for bottom weapon
                bb.image_angle = image_angle;                //rotate the bullet sprite        
                bb.direction = image_angle;                       //set the direction the bullet will move        
                bb.speed = 20;                                             //set the speed the bullet will move
                
                //// END ////
                
                audio_play_sound(snd_player_shoot,0,false);
                }
                
                
            }
            
        else
            {
            player_is_firing = false;
            }
        
        }
    }


