            <kind>1</kind>
            <string>/// Sprite Ghosting Steps
// Update latest trail coordinates and alpha
spr_trail_x[spr_trail_num]=x
spr_trail_y[spr_trail_num]=y
spr_trail_a[spr_trail_num]=1
spr_trail_num+=1

